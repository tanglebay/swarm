#!/bin/bash

parser="$(jq '.network.maxUnknownPeers' $beeHome/$beeConfig.json)"
if [ "$parser" != "$beeMaxUnknownPeers" ]; then
    sudo jq '.network.maxUnknownPeers = '$beeMaxUnknownPeers'' $beeHome/$beeConfig.json|sponge $beeHome/$beeConfig.json
    restartBee=true
    echo "beeMaxUnknownPeers" >> /tmp/beeParsing
fi

unset parser
