#!/bin/bash

parser=$(jq '.inx.address' $inxIndexerHome/$inxIndexerConfig.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$inxIndexerInxAddress" ]; then
    sudo jq '.inx.address = "'$inxIndexerInxAddress'"' $inxIndexerHome/$inxIndexerConfig.json|sponge $inxIndexerHome/$inxIndexerConfig.json
    restartInxIndexer=true
    echo "inxIndexerInxAddress" >> /tmp/inxIndexerParsing
fi
