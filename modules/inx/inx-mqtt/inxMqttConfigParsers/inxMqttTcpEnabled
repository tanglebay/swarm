#!/bin/bash

parser=$(jq '.mqtt.tcp.enabled' $inxIndexerHome/$inxIndexerConfig.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$inxIndexerTcpEnabled" ]; then
    sudo jq '.mqtt.tcp.enabled = '$inxIndexerTcpEnabled'' $inxIndexerHome/$inxIndexerConfig.json|sponge $inxIndexerHome/$inxIndexerConfig.json
    restartInxIndexer=true
    echo "inxIndexerTcpEnabled" >> /tmp/inxIndexerParsing
fi
