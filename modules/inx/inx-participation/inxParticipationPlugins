#!/bin/bash

if [ "$useInxParticipationEnablePlugins" = "true" ]; then
    inxParticipationPlugins=$inxParticipationEnablePlugins
fi
if [ "$useInxParticipationDisablePlugins" = "true" ]; then
    inxParticipationPlugins=$inxParticipationDisablePlugins
fi

### PLUGINS ###

# Prometheus
if [[ $inxParticipationPlugins = *prometheus* ]]; then
    inxParticipationPluginPrometheus=ON
else
    inxParticipationPluginPrometheus=OFF
fi


unset inxParticipationPlugins