#!/bin/bash

source $shimmerInxParticipationModules/inxParticipationConfigs

# Run Config checks
source $shimmerInxParticipationModules/inxParticipationConfigCheck

if [ -z "$shimmerInxParticipationVersion" ]; then
    source $shimmerInxParticipationModules/inxParticipationVersion
fi

#############################################################################################

source $shimmerInxParticipationConfigParsers/inxParticipationDisablePlugins

source $shimmerInxParticipationConfigParsers/inxParticipationEnablePlugins

source $shimmerInxParticipationConfigParsers/inxParticipationInxAddress

source $shimmerInxParticipationConfigParsers/inxParticipationUpdateCheck

source $shimmerInxParticipationConfigParsers/inxParticipationProfiling

# Set file permissions
sudo chown inx:inx $shimmerInxParticipationHome/*.json > /dev/null 2>&1

# Unset all var
unset parser
