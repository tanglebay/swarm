#!/bin/bash

if [ "$useInxDashboardEnablePlugins" = "true" ]; then
    inxDashboardPlugins=$inxDashboardEnablePlugins
fi
if [ "$useInxDashboardDisablePlugins" = "true" ]; then
    inxDashboardPlugins=$inxDashboardDisablePlugins
fi

### PLUGINS ###

# Prometheus
if [[ $inxDashboardPlugins = *prometheus* ]]; then
    inxDashboardPluginPrometheus=ON
else
    inxDashboardPluginPrometheus=OFF
fi


unset inxDashboardPlugins