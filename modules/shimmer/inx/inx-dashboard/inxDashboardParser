#!/bin/bash

source $shimmerInxDashboardModules/inxDashboardConfigs

# Run Config checks
source $shimmerInxDashboardModules/inxDashboardConfigCheck

if [ -z "$shimmerInxDashboardVersion" ]; then
    source $shimmerInxDashboardModules/inxDashboardVersion
fi

#############################################################################################

source $shimmerInxDashboardConfigParsers/inxDashboardDisablePlugins

source $shimmerInxDashboardConfigParsers/inxDashboardEnablePlugins

source $shimmerInxDashboardConfigParsers/inxDashboardInxAddress

source $shimmerInxDashboardConfigParsers/inxDashboardUpdateCheck

source $shimmerInxDashboardConfigParsers/inxDashboardProfiling

# Set file permissions
sudo chown inx:inx $shimmerInxDashboardHome/*.json > /dev/null 2>&1

# Unset all var
unset parser
