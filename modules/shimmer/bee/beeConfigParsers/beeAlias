#!/bin/bash

parser="$(jq '.alias' $beeHome/$beeConfig.json)"
parser=$(echo $parser | tr -d '"')
if [ "$beeAlias" != "$parser" ]; then
    if [ ! -z "$beeAlias" ]; then
        sudo jq --arg beeAlias "$beeAlias" '.alias = ($beeAlias)' $beeHome/$beeConfig.json|sponge $beeHome/$beeConfig.json
        restartBee=true
        newBeeAlias=true
        echo "beeAlias" >> /tmp/beeParsing
    fi
fi
