#!/bin/bash

parser="$(jq '.snapshot.intervalSynced' $beeHome/$beeConfig.json)"
if [ "$parser" != "$beeSnapshotInterval" ]; then
    sudo jq '.snapshot.intervalSynced = '$beeSnapshotInterval'' $beeHome/$beeConfig.json|sponge $beeHome/$beeConfig.json
    restartBee=true
    echo "beeSnapshotInterval" >> /tmp/beeParsing
fi

unset parser
