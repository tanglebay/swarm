#!/bin/bash

parser="$(jq '.dashboard.auth.passwordHash' $beeHome/$beeConfig.json)"
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$beePasswordHash" ]; then
    sudo jq '.dashboard.auth.passwordHash = "'$beePasswordHash'"' $beeHome/$beeConfig.json|sponge $beeHome/$beeConfig.json
    restartBee=true
    echo "beePasswordHash" >> /tmp/beeParsing
fi

unset parser
