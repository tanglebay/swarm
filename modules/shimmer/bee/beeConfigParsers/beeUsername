#!/bin/bash

parser="$(jq '.dashboard.auth.user' $beeHome/$beeConfig.json)"
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$beeUsername" ]; then
    sudo jq '.dashboard.auth.user = "'$beeUsername'"' $beeHome/$beeConfig.json|sponge $beeHome/$beeConfig.json
    restartBee=true
    newBeeUsername=true
    echo "beeUsername" >> /tmp/beeParsing
fi

unset parser
