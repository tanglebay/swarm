#!/bin/bash

parser=$(jq '.app.checkForUpdates' $inxDashboardHome/$inxDashboardConfig.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$inxDashboardUpdateCheck" ]; then
    sudo jq '.app.checkForUpdates = '$inxDashboardUpdateCheck'' $inxDashboardHome/$inxDashboardConfig.json|sponge $inxDashboardHome/$inxDashboardConfig.json
    restartInxDashboard=true
    echo "inxDashboardUpdateCheck" >> /tmp/inxDashboardParsing
fi
