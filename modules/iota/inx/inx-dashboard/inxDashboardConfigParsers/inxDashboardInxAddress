#!/bin/bash

parser=$(jq '.inx.address' $inxDashboardHome/$inxDashboardConfig.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$inxDashboardInxAddress" ]; then
    sudo jq '.inx.address = "'$inxDashboardInxAddress'"' $inxDashboardHome/$inxDashboardConfig.json|sponge $inxDashboardHome/$inxDashboardConfig.json
    restartInxDashboard=true
    echo "inxDashboardInxAddress" >> /tmp/inxDashboardParsing
fi
