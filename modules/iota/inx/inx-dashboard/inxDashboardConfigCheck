#!/bin/bash

if [ ! -s "$inxDashboardHome/$inxDashboardConfig.json" ]; then
    if [ -f "$inxDashboardHome/$inxDashboardConfig.json" ]; then
        sudo chown inx:inx $inxDashboardHome/$inxDashboardConfig.json > /dev/null 2>&1
    fi
    sudo -u inx wget -q -O $inxDashboardHome/$inxDashboardConfig.json https://raw.githubusercontent.com/iotaledger/inx-dashboard/develop/$inxDashboardConfig.json
fi

if [ -f "/tmp/inxDashboardParsing" ]; then
    sudo rm -rf /tmp/inxDashboardParsing > /dev/null 2>&1
fi