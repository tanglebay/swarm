#!/bin/bash

if [ "$useInxMqttEnablePlugins" = "true" ]; then
    inxMqttPlugins=$inxMqttEnablePlugins
fi
if [ "$useInxMqttDisablePlugins" = "true" ]; then
    inxMqttPlugins=$inxMqttDisablePlugins
fi

### PLUGINS ###

# Prometheus
if [[ $inxMqttPlugins = *prometheus* ]]; then
    inxMqttPluginPrometheus=ON
else
    inxMqttPluginPrometheus=OFF
fi


unset inxMqttPlugins