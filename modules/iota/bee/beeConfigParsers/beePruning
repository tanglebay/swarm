#!/bin/bash

parser="$(jq '.pruning.enabled' $beeHome/$beeConfig.json)"
if [ "$parser" != "$beePruningEnabled" ]; then
    sudo jq '.pruning.enabled = '$beePruningEnabled'' $beeHome/$beeConfig.json|sponge $beeHome/$beeConfig.json
    restartBee=true
    echo "beePruning1" >> /tmp/beeParsing
fi
parser="$(jq '.pruning.delay' $beeHome/$beeConfig.json)"
if [ "$parser" != "$beePruningDelay" ]; then
    sudo jq '.pruning.delay = '$beePruningDelay'' $beeHome/$beeConfig.json|sponge $beeHome/$beeConfig.json
    restartBee=true
    echo "beePruning2" >> /tmp/beeParsing
fi

unset parser
