#!/bin/bash

if [ "$useInxIndexerEnablePlugins" = "true" ]; then
    inxIndexerPlugins=$inxIndexerEnablePlugins
fi
if [ "$useInxIndexerDisablePlugins" = "true" ]; then
    inxIndexerPlugins=$inxIndexerDisablePlugins
fi

### PLUGINS ###

# Prometheus
if [[ $inxIndexerPlugins = *prometheus* ]]; then
    inxIndexerPluginPrometheus=ON
else
    inxIndexerPluginPrometheus=OFF
fi


unset inxIndexerPlugins