#!/bin/bash

parser=$(jq '.nanomsg.enabled' $shimmerWaspHome/config.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$shimmerWaspNanomsgEnabled" ]; then
    sudo jq '.nanomsg.enabled = '$shimmerWaspNanomsgEnabled'' $shimmerWaspHome/config.json|sponge $shimmerWaspHome/config.json
    restartShimmerWasp=true
    echo "shimmerWaspNanomsgEnabled" >> /tmp/shimmerWaspParsing
fi