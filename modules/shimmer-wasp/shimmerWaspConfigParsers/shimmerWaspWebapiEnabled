#!/bin/bash

parser=$(jq '.webapi.enabled' $shimmerWaspHome/config.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$shimmerWaspWebapiEnabled" ]; then
    sudo jq '.webapi.enabled = '$shimmerWaspWebapiEnabled'' $shimmerWaspHome/config.json|sponge $shimmerWaspHome/config.json
    restartShimmerWasp=true
    echo "shimmerWaspWebapiEnabled" >> /tmp/shimmerWaspParsing
fi
