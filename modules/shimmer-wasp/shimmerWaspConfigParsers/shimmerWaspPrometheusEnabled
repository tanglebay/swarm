#!/bin/bash

parser=$(jq '.prometheus.enabled' $shimmerWaspHome/config.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$shimmerWaspPrometheusEnabled" ]; then
    sudo jq '.prometheus.enabled = '$shimmerWaspPrometheusEnabled'' $shimmerWaspHome/config.json|sponge $shimmerWaspHome/config.json
    restartShimmerWasp=true
    echo "shimmerWaspPrometheusEnabled" >> /tmp/shimmerWaspParsing
fi
