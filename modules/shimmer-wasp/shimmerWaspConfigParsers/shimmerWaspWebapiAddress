#!/bin/bash

parser=$(jq '.webapi.bindAddress' $shimmerWaspHome/config.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "localhost:9091" ]; then
    sudo jq '.webapi.bindAddress = "localhost:9091"' $shimmerWaspHome/config.json|sponge $shimmerWaspHome/config.json
    restartShimmerWasp=true
    echo "shimmerWaspWebapiAddress" >> /tmp/shimmerWaspParsing
fi
