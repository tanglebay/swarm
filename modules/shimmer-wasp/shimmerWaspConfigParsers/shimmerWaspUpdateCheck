#!/bin/bash

parser=$(jq '.app.checkForUpdates' $shimmerWaspHome/config.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$shimmerWaspUpdateCheck" ]; then
    sudo jq '.app.checkForUpdates = '$shimmerWaspUpdateCheck'' $shimmerWaspHome/config.json|sponge $shimmerWaspHome/config.json
    restartShimmerWasp=true
    echo "shimmerWaspUpdateCheck" >> /tmp/shimmerWaspParsing
fi
