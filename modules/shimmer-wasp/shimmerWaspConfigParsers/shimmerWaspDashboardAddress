#!/bin/bash

parser=$(jq '.dashboard.bindAddress' $shimmerWaspHome/config.json)
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "localhost:7001" ]; then
    sudo jq '.dashboard.bindAddress = "localhost:7001"' $shimmerWaspHome/config.json|sponge $shimmerWaspHome/config.json
    restartShimmerWasp=true
fi