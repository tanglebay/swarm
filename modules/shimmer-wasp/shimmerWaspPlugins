#!/bin/bash

if [ "$useShimmerWaspEnablePlugins" = "true" ]; then
    shimmerWaspPlugins=$shimmerWaspEnablePlugins
fi
if [ "$useShimmerWaspDisablePlugins" = "true" ]; then
    shimmerWaspPlugins=$shimmerWaspDisablePlugins
fi

### PLUGINS ###

# Nanomsg
if [[ $shimmerWaspPlugins = *nanomsg* ]]; then
    shimmerWaspNanomsg=ON
else
    shimmerWaspNanomsg=OFF
fi

unset shimmerWaspPlugins