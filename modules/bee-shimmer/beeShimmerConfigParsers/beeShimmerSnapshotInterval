#!/bin/bash

parser="$(jq '.snapshot.intervalSynced' $beeShimmerHome/$beeShimmerConfig.json)"
if [ "$parser" != "$beeShimmerSnapshotInterval" ]; then
    sudo jq '.snapshot.intervalSynced = '$beeShimmerSnapshotInterval'' $beeShimmerHome/$beeShimmerConfig.json|sponge $beeShimmerHome/$beeShimmerConfig.json
    restartBeeShimmer=true
    echo "beeShimmerSnapshotInterval" >> /tmp/beeShimmerParsing
fi

unset parser
