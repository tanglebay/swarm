#!/bin/bash

parser="$(jq '.dashboard.auth.passwordHash' $beeShimmerHome/config.chrysalis-$beeShimmerNetwork.json)"
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$beeShimmerPasswordHash" ]; then
    sudo jq '.dashboard.auth.passwordHash = "'$beeShimmerPasswordHash'"' $beeShimmerHome/config.chrysalis-$beeShimmerNetwork.json|sponge $beeShimmerHome/config.chrysalis-$beeShimmerNetwork.json
    restartBeeShimmer=true
    echo "beeShimmerPasswordHash" >> /tmp/beeShimmerParsing
fi

unset parser
