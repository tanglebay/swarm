#!/bin/bash

source $swarmConfigs/bee-shimmer.cfg

if [ "$beeShimmerNetwork" = "mainnet" ]; then
    beeShimmerConfig="config.chrysalis-mainnet"
fi

if [ "$beeShimmerNetwork" = "comnet" ]; then
    beeShimmerConfig="config.chrysalis-comnet"
fi

if [ "$beeShimmerNetwork" = "devnet" ]; then
    beeShimmerConfig="config.chrysalis-devnet"
fi

if [ "$beeShimmerNetwork" = "alphanet" ]; then
    beeShimmerConfig="config.alphanet"
fi