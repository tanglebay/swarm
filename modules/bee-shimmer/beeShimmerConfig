#!/bin/bash

source $swarmConfigs/bee-shimmer.cfg

if [ "$beeShimmerNetwork" = "mainnet" ]; then
    beeShimmerConfig="config.chrysalis-mainnet.json"
fi

if [ "$beeShimmerNetwork" = "comnet" ]; then
    beeShimmerConfig="config.chrysalis-comnet.json"
fi

if [ "$beeShimmerNetwork" = "devnet" ]; then
    beeShimmerConfig="config.chrysalis-devnet.json"
fi