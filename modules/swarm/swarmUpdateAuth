#!/bin/bash

if [ "$swarmUpdateAuthValid" != "200" ]; then
    swarmUpdateAuthValid=$(curl -s -o /dev/null -w "%{http_code}" https://$swarmUpdateAuthUser:$swarmUpdateAuthPwd@service.tanglebay.com/download/swarm/latest/checksum.txt)
fi

if [ "$swarmUpdateAuthValid" = "200" ]; then
    swarmUpdateAuthValidText="valid"
else
    swarmUpdateAuthValidText="invalid"
fi