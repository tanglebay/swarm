#!/bin/bash

parser="$(jq '.pruning.milestones.enabled' $iotaNodePath/config.json)"
if [ "$parser" != "false" ]; then
    sudo jq '.pruning.milestones.enabled = false' $iotaNodePath/config.json|sponge $iotaNodePath/config.json
    restartIotaNode=true
    echo "iotaNodePruningMilestones" >> /tmp/iotaNodeParsing
fi