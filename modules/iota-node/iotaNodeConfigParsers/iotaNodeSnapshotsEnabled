#!/bin/bash

parser="$(jq '.snapshots.enabled' $iotaNodePath/config.json)"
if [ "$iotaNodeSnapshotsEnabled" = "true" ] || [ "$iotaNodeSnapshotsEnabled" = "false" ]; then
    if [ "$parser" != "$iotaNodeSnapshotsEnabled" ]; then
        sudo jq '.snapshots.enabled = '$iotaNodeSnapshotsEnabled'' $iotaNodePath/config.json|sponge $iotaNodePath/config.json
        restartIotaNode=true
        echo "iotaNodeSnapshotsEnabled" >> /tmp/iotaNodeParsing
    fi
fi