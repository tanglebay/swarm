#!/bin/bash

proxyIotaHornetDomain=$(echo $proxyIotaHornetUrl | sed 's/:.*//')
proxyIotaHornetPort=$(echo $proxyIotaHornetUrl | sed 's/.*://')
proxyShimmerHornetDomain=$(echo $proxyShimmerHornetUrl | sed 's/:.*//')
proxyShimmerHornetPort=$(echo $proxyShimmerHornetUrl | sed 's/.*://')

proxyIotaInxDashboardDomain=$(echo $proxyIotaInxDashboardUrl | sed 's/:.*//')
proxyIotaInxDashboardPort=$(echo $proxyIotaInxDashboardUrl | sed 's/.*://')
proxyShimmerInxDashboardDomain=$(echo $proxyShimmerInxDashboardUrl | sed 's/:.*//')
proxyShimmerInxDashboardPort=$(echo $proxyShimmerInxDashboardUrl | sed 's/.*://')

proxyGoshimmerDomain=$(echo $proxyGoshimmerUrl | sed 's/:.*//')
proxyGoshimmerPort=$(echo $proxyGoshimmerUrl | sed 's/.*://')

proxyShimmerWaspDashboardDomain=$(echo $proxyShimmerWaspDashboardUrl | sed 's/:.*//')
proxyShimmerWaspDashboardPort=$(echo $proxyShimmerWaspDashboardUrl | sed 's/.*://')
proxyShimmerWaspApiDomain=$(echo $proxyShimmerWaspApiUrl | sed 's/:.*//')
proxyShimmerWaspApiPort=$(echo $proxyShimmerWaspApiUrl | sed 's/.*://')