#!/bin/bash

source $inxParticipationModules/inxParticipationConfigs

# Run Config checks
source $inxParticipationModules/inxParticipationConfigCheck

if [ -z "$inxParticipationVersion" ]; then
    source $inxParticipationModules/inxParticipationVersion
fi

#############################################################################################

source $inxParticipationConfigParsers/inxParticipationDisablePlugins

source $inxParticipationConfigParsers/inxParticipationEnablePlugins

source $inxParticipationConfigParsers/inxParticipationInxAddress

source $inxParticipationConfigParsers/inxParticipationUpdateCheck

source $inxParticipationConfigParsers/inxParticipationProfiling

# Set file permissions
sudo chown inx:inx $inxParticipationHome/*.json > /dev/null 2>&1

# Unset all var
unset parser
