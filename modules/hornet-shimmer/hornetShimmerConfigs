#!/bin/bash

source $swarmConfigs/hornet-shimmer.cfg

if [ "$hornetShimmerNetwork" = "mainnet" ]; then
    hornetShimmerConfig="config"
fi

if [ "$hornetShimmerNetwork" = "comnet" ]; then
    hornetShimmerConfig="config_comnet"
fi

if [ "$hornetShimmerNetwork" = "devnet" ]; then
    hornetShimmerConfig="config_devnet"
fi

if [ "$hornetShimmerNetwork" = "alphanet" ]; then
    hornetShimmerConfig="config"
fi